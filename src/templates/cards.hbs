{{#each this}}
  <li class='column'>
    <article class='photo-card container'>
      <img
        class='item-image image'
        src={{webformatURL}}
        alt={{tags}}
        data-source={{webformatURL}}
      />
      <ul class='overlay flex'>

        <li class='stats-item'>
          <i class='material-icons'>thumb_up</i>
          {{likes}}
        </li>

        <li class='stats-item'>
          <i class='material-icons'>visibility</i>
          {{views}}
        </li>

        <li class='stats-item'>
          <i class='material-icons'>comment</i>
          {{comments}}
        </li>

        <li class='stats-item'>
          <i class='material-icons'>cloud_download</i>
          {{downloads}}
        </li>

      </ul>
    </article>
  </li>
{{/each}}